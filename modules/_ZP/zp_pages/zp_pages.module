/**
 * 
 * @param array &$form
 *   Array with declared form.
 *
 * @param array &$form_state
 *   Array with current form state.

 * @return void
 *   Return nothing.
 */
function zp_pages_importPrices_formSubmit(&$form, &$form_state) {

  //dpm($_POST);
  //dpm($form_state);
  
  $file_data = array();
  $get = $_GET;
  unset($get['q'], $get['action']);
  
  // just trying to *pre-populate a form in D7* after submit/rebuild, and it took forever to figure out how!
  // https://www.drupal.org/node/1395992
  $form_state['rebuild'] = TRUE;
          
  //form_set_value($form['files'], array('und' => array(0 => array('value' => $form_state['values']['files']) )), $form_state);
  //form_set_value($form['files'], $form_state['values']['files'], $form_state);
  
  if ($form_state['values']['op'] == 'Загрузить новый прайс-лист') {
    
    //$get['action'] = 'done';
    
    $file = file_load($form_state['values']['file_fid']);
    //dpm($file);

    // Change status to permanent.
    $file->status = FILE_STATUS_PERMANENT;
    // Save.
    $uploaded = file_save($file);
    
    if ($uploaded == TRUE) {
      drupal_set_message('The file has been uploaded.');
    } // End of if ($uploaded == TRUE) {
    
  } // if ($form_state['values']['op'] == 'Import from file') {
  elseif ( $form_state['values']['op'] == 'Проверить выбранный прайс-лист') {
    drupal_set_message('Task: test a price-list');
    //dpm($form_state['values']);
    $file = file_load($form_state['values']['files']);
      
    if ($file_data = zp_pages_getPriceFromFile($file)) {
      zp_pages_importPriceBatch('test', $file_data, $file);
    }
  }
  elseif ( $form_state['values']['op'] == 'Импортировать/применить выбранный прайс-лист') {
    drupal_set_message('Task: import/apply a price-list');
    //dpm($form_state['values']);
  }
  elseif ( $form_state['values']['op'] == 'Показать результаты операции по выбранному файлу') {
    drupal_set_message('Task: Показать результаты операции по выбранному файлу');
    //dpm($form_state['values']);
    
    if (empty($form_state['values']['files'])) {
      drupal_set_message('Выберите файл для вывода результатов по нему.', 'warning');
    }
    else {
      $query = db_select('zp_price_import_results', 'r')
        ->fields('r', array('fid', 'filename', 'batch_id', 'batch_finished', 'op', 'data'));
      
      $query->leftJoin('batch', 'b', 'b.bid = r.batch_id');
      $query->fields('b', array('bid'));
      
      $query->condition('r.fid', $form_state['values']['files']);
      
      if ($GLOBALS['user']->uid != 1) {
        $query->condition('r.uid', $GLOBALS['user']->uid);
      }

      $result = $query->execute()->fetchObject();
      
      dpm('Результаты выполнения операции ' . $result->op . ' над файлом ' . $result->filename . ':');
      if (!$result) {
        dpm('Нет данных по файлу ' . $result->filename, 'warning');
      }
      else {
        $data = unserialize($result->data);
        unset($data['objects_source']);
        //$data['objects_source'] = 'Array, just hidden here... look into DB for it'.
        //unset($data['objects_processed']);
        dpm($data);
      }
    }
    
  }
  elseif ( $form_state['values']['op'] == 'Удалить выбранную незавершенную операцию') {
    drupal_set_message('Task: Удалить выбранную незавершенную операцию');
    //dpm($form_state['values']);
    if (empty($form_state['values']['files'])) {
      drupal_set_message('Выберите файл для удаления незавершенной операции до нажатия на кнопку удаления.', 'warning');
    }
    else {
      $query = db_select('zp_price_import_results', 'r')
        ->fields('r', array('fid', 'filename', 'batch_id', 'batch_finished', 'op'));
      
      $query->leftJoin('batch', 'b', 'b.bid = r.batch_id');
      $query->fields('b', array('bid'));
      
      $query->condition('r.fid', $form_state['values']['files']);
      $query->condition('r.batch_finished', 0);
      
      if ($GLOBALS['user']->uid != 1) {
        $query->condition('r.uid', $GLOBALS['user']->uid);
      }

      $result = $query->execute()->fetchObject();
      
      if (!$result->bid) {
        drupal_set_message('Нечего удалять! Операция уже была завершена или отменена/удалена', 'warning');
      }
      else {
        db_delete('batch')
        ->condition('bid', $result->bid)
        ->execute();
        drupal_set_message('Незавершенная операция ' . $result->bid . ' для файла ' . $result->filename . ' (fid: ' . $result->fid . ') была удалена.', 'warning');
      }
    }
  }
  elseif ( $form_state['values']['op'] == 'Удалить выбранный прайс-лист') {
    drupal_set_message('Task: delete a file...');
    //dpm($form_state['values']);
    if (empty($form_state['values']['files'])) {
      drupal_set_message('Выберите файл для удаления до нажатия на кнопку удаления.', 'warning');
    }
    else {
      $file = file_load($form_state['values']['files']);

      // https://api.drupal.org/api/drupal/includes%21file.inc/function/file_delete/7
      // https://www.drupal.org/node/2334293
      file_delete($file);
      $filename = explode('/', $file->uri);
      $filename = $filename[count($filename) - 1];
      drupal_set_message('The file ' . $filename . ' has been deleted.');
    }
  }
  
  drupal_goto($_GET['q'], array('query' => $get));
}


